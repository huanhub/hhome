{
  "env":{
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
   },
  "parser": "babel-eslint",
  "parserOptions":  {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
    },
  },
  "extends": ["google", "plugin:react/recommended"],
  "rules": {
      "require-jsdoc": 0,
      "no-invalid-this": 0,
  }
}
